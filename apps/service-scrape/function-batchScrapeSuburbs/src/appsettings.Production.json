{
  "Serilog": {
    "Using": ["Serilog.Sinks.OpenTelemetry"],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "OpenTelemetry",
        "Args": {
          "endpoint": "https://api.axiom.co/v1/traces",
          "protocol": "HttpProtobuf"
        }
      }
    ],
    "Enrich": ["WithMachineName", "WithThreadName", "WithThreadId"]
  }
}
