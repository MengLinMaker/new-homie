{
    "name": "@observability/lib-grafana",
    "private": true,
    "scripts": {
        "format": "biome format --write",
        "test:type": "tsc",
        "test:unit": "vitest run --coverage",
        "test:perf": "vitest bench",
        "test:dev": "vitest --coverage",
        "generate": "openapi-typescript --array-length true --properties-required-by-default true './src/api/openapi.json' -o './src/api/schema.d.ts' && biome format --write",
        "grafana:export": "tsx ./src/export.ts && biome format --write",
        "grafana:import": "tsx ./src/import.ts"
    },
    "type": "module",
    "devDependencies": {
        "openapi-fetch": "^0.14.0",
        "openapi-typescript": "^7.9.1",
        "zod": "4.0.17"
    }
}
