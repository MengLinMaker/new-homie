{
    "transforms": [
        {
            "mode": "sql",
            "include": "**/*.sql",
            "emitTemplate": "{{dir}}/{{name}}.sql.ts"
        },
        {
            "mode": "ts",
            "include": "**/*.ts",
            "emitTemplate": "{{dir}}/{{name}}.types.ts"
        }
    ],
    "srcDir": "./src/",
    "dbUrl": "postgres://user:password@localhost:54320/db"
}
