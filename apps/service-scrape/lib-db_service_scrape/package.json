{
    "name": "@service-scrape/lib-db_service_scrape",
    "version": "1.0.0",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "format": "biome format --write",
        "docker:down": "docker compose down -v",
        "migrate": "node ./migrate.ts | pnpm dlx pino-pretty",
        "test": "pnpm run /^test:/",
        "test:lint": "biome lint --write",
        "test:type": "tsc",
        "test:unit": "vitest run --coverage",
        "dev:test:unit": "vitest --coverage",
        "build": "tsup src/index.ts --format esm --dts --clean --sourcemap"
    },
    "dependencies": {
        "dotenv": "16.4.7",
        "kysely": "0.27.5",
        "pg": "8.13.3",
        "pino": "^9.7.0",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@faker-js/faker": "9.5.0",
        "@types/pg": "8.11.11",
        "kysely-codegen": "^0.18.5",
        "pino-pretty": "^13.1.1"
    },
    "type": "module"
}
